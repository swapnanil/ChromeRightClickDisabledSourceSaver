{
  "name": "Chrome page source saver",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Get pages source from a popup",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "<all_urls>", "http://*/", "https://*/"]
}